<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseListPreference" >
        <attr name="entries" format="reference"/>
        <attr name="entryValues" format="reference" />
        <attr name="default_value" format="string" />
    </declare-styleable>
    <declare-styleable name="PagePointLayout">
        <attr name="point_count" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TabBarLayout">
        <attr name="infos" format="reference"/>
        <attr name="icons" format="reference"/>
        <attr name="tabLayout" format="reference"/>
    </declare-styleable>
    <declare-styleable name="reflect">
        <attr name="reflectAlpha" format="integer"/>
        <attr name="reflectHeightMultiple" format="float"/>
        <attr name="spacingValue" format="integer"/>
    </declare-styleable>

    <declare-styleable name="VerticalScrollTextView">
        <attr name="animationDuration" format="integer"/>
        <attr name="maxLines" format="integer"/>
        <attr name="textStillTime" format="integer"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="MarqueeTextView">
            <attr name="scroll_interval" format="integer" />
            <attr name="scroll_first_delay" format="integer"/>
            <attr name="scroll_mode">
                <enum name="mode_forever" value="100"/>
                <enum name="mode_once" value="101"/>
            </attr>
    </declare-styleable>

</resources>